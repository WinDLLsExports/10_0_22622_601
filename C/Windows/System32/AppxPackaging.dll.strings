    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ST_Executable">
    <xs:restriction base="ST_FileName">
      <xs:pattern value=".+\.(exe)"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ST_ExecutableNoPath">
