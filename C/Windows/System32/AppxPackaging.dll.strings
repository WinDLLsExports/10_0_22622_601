  <xs:simpleType name="ST_MutableDirectoryTarget">
    <xs:restriction base="ST_FileNameCharSetAndColon">
      <xs:pattern value="\$\(([\.\w])*\)(\\[^/\\]*[^./\\]+)+(\\?)"/>
