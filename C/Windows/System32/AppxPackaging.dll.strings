    <xs:restriction base="xs:string">
      <xs:enumeration value="application" />
      <xs:enumeration value="resource" />
    </xs:restriction>
  </xs:simpleType>
  <!-- Mutable directory target type enforces that the directory must be of form $(string)\subpath -->
