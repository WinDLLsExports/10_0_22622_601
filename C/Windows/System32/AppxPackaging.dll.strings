      <xs:pattern value=".+\.([Ee][Xx][Ee])"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ST_DllFile">
    <xs:restriction base="ST_FileName">
      <xs:pattern value=".+\.([Dd][Ll][Ll])"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ST_Dll_EXE_File">
    <xs:restriction base="ST_FileName">
      <xs:pattern value=".+\.(([Dd][Ll][Ll])|([Ee][Xx][Ee]))"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ST_ImageFile">
    <xs:restriction base="ST_FileName">
      <xs:pattern value=".+\.((jpg)|(png)|(jpeg))"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ST_ImageLogo">
    <xs:restriction base="ST_FileName">
      <xs:pattern value=".+\.(([Jj][Pp][Gg])|([Pp][Nn][Gg])|([Jj][Pp][Ee][Gg])|([Ii][Cc][Oo])|([Dd][Ll][Ll])|([Ee][Xx][Ee]))"/>
    </xs:restriction>
  </xs:simpleType>
