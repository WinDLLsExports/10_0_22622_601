            </xs:unique>
          </xs:element>
          <xs:element name="Verbs" type="CT_Verbs" minOccurs="0">
            <xs:unique name="Verb_Index">
              <xs:selector xpath="com:Verb"/>
              <xs:field xpath="@Id"/>
            </xs:unique>
          </xs:element>
          <xs:element name="DefaultIcon" type="CT_DefaultIcon" minOccurs="0"/>
