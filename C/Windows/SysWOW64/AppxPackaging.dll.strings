    <xs:complexContent>
      <xs:extension base="CT_Class">
        <xs:all>
          <xs:element name="ImplementedCategories" type="CT_ImplementedCategories" minOccurs="0">
            <xs:unique name="ImplementedCategory_Id">
              <xs:selector xpath="com:ImplementedCategory"/>
