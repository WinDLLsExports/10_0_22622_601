  <xsd:simpleType name="NonEmptyListOfParameterSetNames">
    <xsd:restriction>
      <xsd:simpleType>
        <xsd:list itemType="cps:ParameterSetName" />
      </xsd:simpleType>
      <xsd:minLength value="1" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="NonEmptyListOfAliases">
    <xsd:restriction>
      <xsd:simpleType>
        <xsd:list itemType="cps:Alias" />
