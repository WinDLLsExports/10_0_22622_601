    colorHsl.z = dot(colorRgb, g_luminosityWeights);
    // S
    colorHsl.y = flMax - flMin;
    // H
    colorHsl.x = (colorHsl.y == 0.0) ?
        0.0 :
        (flMax == colorRgb.r) ?
        ((colorRgb.g - colorRgb.b) / colorHsl.y) :
        (flMax == colorRgb.g) ?
        (2.0 + (colorRgb.b - colorRgb.r) / colorHsl.y) :
        (4.0 + (colorRgb.r - colorRgb.g) / colorHsl.y);
