        data:
          allow, block, mapipin, mapipout, mapmacin, mapmacout, pingresponse, caredirect: 
          mapencap: [space] [mss_del]/[rev_mss_del] [src_pa] [key] [flags] [[local_cert_thumbprint] [remote_cert_subject_name]] [[second src_pa]]
