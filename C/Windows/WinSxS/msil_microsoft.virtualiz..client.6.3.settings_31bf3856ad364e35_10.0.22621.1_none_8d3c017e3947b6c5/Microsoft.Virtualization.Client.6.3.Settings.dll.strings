            <xs:field xpath="@index"/>
        </xs:key>
        <xs:keyref name="LabelOwnerKeyRef" refer="NameKey">
            <xs:selector xpath=".//label|.//dataLabel"/>
            <xs:field xpath="@owner"/>
        </xs:keyref>
        <xs:keyref name="RadioButtonKeyRef" refer="RadioButtonSetKey">
