        <xs:key name="NameKey">
            <!--Anytime a new control is added they should be added to this xpath expression to make sure their name is unique.-->
            <xs:selector xpath=".//label|.//helpLinkLabel|.//dataLabel|.//imagePlusLabel|.//checkBox|.//comboBox|.//listBox|.//checkedListBox|
            .//dataGridView|.//authorizationEntryDataGridView|.//portListDataGridView|.//physicalGPUVMListGridView|.//switchExtensionListDataGridView|.//firmwareBootOrderDataGridView|.//certificateControl|
